class Solution {
    public String generateTheString(int n) {
        String out = "";
        if(n%2!=0){
            for(int i = 0; i<n; i++){
                out+="a";
            }   
            return out;
        }
        
        int num = n;
        
        for(int i = 0; i<n-1; i++){
            out+="a";
        }
        out+="n";
        return out;
        
    }
}
