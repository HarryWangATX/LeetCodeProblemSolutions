class Solution {
    public int countElements(int[] arr) {
        int count = 0;
        for(int i = 0; i<arr.length; i++){
            for(int j = 0; j<arr.length; j++){
                if(arr[i] + 1 == arr[j]){
                    count++;
                    break;
                }
            }
        }
        return count;
    }
}
