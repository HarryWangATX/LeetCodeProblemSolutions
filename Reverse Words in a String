class Solution {
    public String reverseWords(String s) {
        String[] arr = s.split(" ");
        String res = "";
        for(int i = arr.length-1; i>=0; i--){
            if(arr[i].length() == 0){
                continue;
            }
            res += arr[i];
            
            res += " ";
        }
        if(res.length() == 0){
            return "";
        }
        return res.substring(0, res.length()-1);
    }
}
